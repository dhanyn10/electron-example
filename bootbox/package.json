{
  "name": "electron-with-bootbox",
  "version": "0.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder build",
    "test": "mocha"
  },
  "build": {
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb"
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ]
    }
  },
  "repository": "https://github.com/dhanyn10/electron-example",
  "keywords": [
    "tutorial",
    "demo",
    "bootbox",
    "bootstrap",
    "jquery"
  ],
  "author": "Dhany Nurdiansyah",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^11.4.3",
    "electron-builder": "^22.10.4",
    "mocha": "^8.3.2",
    "spectron": "^13.0.0"
  },
  "dependencies": {
    "bootbox": "^5.4.0",
    "bootstrap": "^3.3.7"
  }
}
